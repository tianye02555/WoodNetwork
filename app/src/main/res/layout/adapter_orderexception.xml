<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/activity_order_exception_text_yichangxinxi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/distance_normal"
        android:layout_marginTop="@dimen/distance_normal"
        android:text="发错货"
        android:textColor="@android:color/black"
        android:textSize="@dimen/font_normal" />

    <woodnetwork.hebg3.com.woodnetwork.view.MyGridView
        android:id="@+id/activity_order_exception_gridview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:verticalSpacing="@dimen/distance_normal"
        android:horizontalSpacing="@dimen/distance_normal"
        android:layout_marginTop="@dimen/distance_normal"
        android:numColumns="4"></woodnetwork.hebg3.com.woodnetwork.view.MyGridView>
</LinearLayout>