<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="woodnetwork.hebg3.com.woodnetwork">

    <application
        android:name=".WoodNetworkApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="sysfunction.activity.LoginActivity"></activity>
        <activity android:name="sysfunction.activity.GetPasswordActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ShoppingMall.activity.ShoppingMallActivity"></activity>
    </application>

</manifest>